{
  "extends": "tslint-config-airbnb",
  "rules": {
    "max-line-length": [
      true,
      140
    ],
    "trailing-comma": [
      false
    ],
    "no-increment-decrement": false,
    "variable-name": [
      "allow-leading-underscore"
    ],
    "import-name": false,
    "function-name": false,
    "no-var-self": false,
    "no-empty": false,
    "no-string-literal": false
  }
}
