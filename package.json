    "eslint": "^3.6.1",
    "grunt": "^1.0.1",
    "grunt-argos-deps": "git+https://git@github.com/Saleslogix/grunt-argos-deps.git#v0.1.4",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-jasmine": "^1.0.3",
    "grunt-contrib-less": "^1.0.1",
