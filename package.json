{
  "name": "argos-sdk",
  "version": "3.7.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Saleslogix/argos-sdk"
  },
  "scripts": {
    "build": "tsc && babel ts-out --out-dir src-out --source-maps inline",
    "build:watch": "echo 'run tsc --watch separately' && tsc && babel ts-out --out-dir src-out --source-maps inline --watch",
    "docs": "typedoc --options typedoc.json --exclude \"**/*.+(spec.ts|test.ts)\" ./src/",
    "less": "grunt less",
    "lint": "tslint \"src/**/*.ts\"",
    "lint-fix": "eslint --fix ./src",
    "test": "npm run build && grunt test",
    "testbasic": "grunt test:basic",
    "watch": "yarn run build:watch"
  },
  "devDependencies": {
    "@types/node": "~7.0.13",
    "@types/jquery": "^2.0.41",
    "babel-cli": "^6.16.0",
    "babel-eslint": "^7.2.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-es2015-modules-amd": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-without-strict": "^0.0.4",
    "eslint": "^3.6.1",
    "grunt": "^1.0.1",
    "grunt-argos-deps": "git+https://git@github.com/Saleslogix/grunt-argos-deps.git#v0.1.4",
    "grunt-contrib-clean": "^1.0.0",
    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-jasmine": "^1.0.3",
    "grunt-contrib-less": "^1.0.1",
    "grunt-shell": "^2.0.0",
    "grunt-template-jasmine-istanbul": "^0.5.0",
    "tslint": "~5.1.0",
    "tslint-config-airbnb": "1.1.1",
    "typedoc": "0.5.10",
    "typescript": "~2.2.2"
  },
  "dependencies": {
    "@infor/icrm-js-common": "^0.1.1",
    "@infor/icrm-js-customization": "^0.1.0",
    "@infor/icrm-js-services": "^0.1.1",
    "@infor/sohoxi": "4.2.6-develop",
    "jquery": "^3.2.1",
    "moment": "^2.17.1",
    "redux": "^3.6.0",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.2.0",
    "rxjs": "^5.0.3"
  }
}
